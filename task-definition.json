{
    "family": "luxe-hair-studio",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::995507554189:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "django-app",
            "image": "995507554189.dkr.ecr.us-east-1.amazonaws.com/containers-image-repository:latest",
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "DEBUG",
                    "value": "False"
                },
                {
                    "name": "BUSINESS_TYPE",
                    "value": "hairdresser"
                },
                {
                    "name": "BUSINESS_NAME",
                    "value": "Luxe Hair Studio"
                },
                {
                    "name": "PROVIDER_TITLE",
                    "value": "Hairdresser"
                },
                {
                    "name": "PROVIDER_TITLE_PLURAL",
                    "value": "Hairdressers"
                },
                {
                    "name": "ALLOWED_HOSTS",
                    "value": "luxe-hair-studio-alb-738301618.us-east-1.elb.amazonaws.com"
                }
            ],
            "secrets": [
                {
                    "name": "SECRET_KEY",
                    "valueFrom": "arn:aws:secretsmanager:us-east-1:995507554189:secret:django-secret-key-TCREPg"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/luxe-hair-studio",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}